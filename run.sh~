./assn_3 --basic ./basic/sm/input.1000.bin ./basic/sm/sorted_basic.bin > 1.txt
diff ./basic/sm/sorted_basic.bin ./basic/sm/sort.bin
echo "Basic Small Done "

./assn_3 --basic ./basic/med/input.30000.bin ./basic/med/sorted_basic.bin > 1.txt
diff ./basic/med/sorted_basic.bin ./basic/med/sort.bin
echo "Basic Medium Done "

./assn_3 --basic ./basic/lg/input.99000.bin ./basic/lg/sorted_basic.bin > 1.txt
diff ./basic/lg/sorted_basic.bin ./basic/lg/sort.bin
echo "Basic Long Done "

