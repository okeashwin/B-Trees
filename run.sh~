echo "Multistep Long Done "

./assn_3 --replacement ./replacement/sm/input.1000.bin ./replacement/sm/sorted_basic.bin > 1.txt
diff ./replacement/sm/sorted_basic.bin ./replacement/sm/sort.bin
echo "Replacement Small Done "

